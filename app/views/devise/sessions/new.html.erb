<section class="cmsBacker parallax2">
	
	<div class="">
		<!-- Your Logo -->
		<%= image_tag('logo-icon-m.png') %>
	</div>
	
	<div class="title grid-full middle">
		<h2 class="bold">Sign in</h2>
		<span class="border"></span>
	</div>
	
	<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
		<div class="formWrapper">	
			<%= devise_error_messages! %>
			<%= f.label :email, class: "formText" %><br/>
    		<%= f.email_field :email, :autofocus => true, class: 'email text-area' %><br /><br />
			<%= f.label :password, class: "formText" %><br />
    		<%= f.password_field :password, class: 'email text-area' %><br /><br />
		</div>
		<div class="title grid-full">
			<%= f.submit "Sign in", class: "button2" %><br/><br/>
		</end>
	<% end %>
</section>
